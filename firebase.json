{
  "firebase": "allegrotime",
  "public": "build",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "headers": [
    {
      "source" : "/data/*.json",
      "headers" : [
        {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        }
      ]
    }
  ]
}
